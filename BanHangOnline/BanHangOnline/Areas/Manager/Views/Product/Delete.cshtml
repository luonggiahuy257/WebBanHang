@model BanHangOnline.ViewModels.ProductViewModel

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Manager/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper-scroll">
    <!-- Content wrapper start -->
    <div class="content-wrapper">
        <!-- Row start -->
        <div class="row gutters">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card">
                    <div class="card-body">
                        <form asp-action="Delete">
                            <div class="table-responsive">
                                <table id="copy-print-csv" class="table v-middle">
                                    <thead>
                                        <tr>
                                            <th> @Html.DisplayNameFor(model => model.ProductName)</th>
                                            <th>@Html.DisplayNameFor(model => model.ShortDescription)</th>
                                            <th>@Html.DisplayNameFor(model => model.Description)</th>
                                            <th>@Html.DisplayNameFor(model => model.ManufacturerId)</th>
                                            <th>@Html.DisplayNameFor(model => model.UnitTypeId)</th>
                                            <th>@Html.DisplayNameFor(model => model.TaxRateId)</th>
                                            <th>@Html.DisplayNameFor(model => model.SalePrice)</th>
                                            <th>@Html.DisplayNameFor(model => model.RetailPrice)</th>
                                            <th>@Html.DisplayNameFor(model => model.Point)</th>
                                            <th>@Html.DisplayNameFor(model => model.ShowOnHomePage)</th>
                                            <th>@Html.DisplayNameFor(model => model.ShowOnSalePage)</th>
                                            <th>@Html.DisplayNameFor(model => model.WarehouseId)</th>
                                            <th>@Html.DisplayNameFor(model => model.ProductTypeId)</th>
                                            <th>@Html.DisplayNameFor(model => model.IsGroup)</th>
                                            <th>@Html.DisplayNameFor(model => model.OwenSale)</th>
                                            <th>@Html.DisplayNameFor(model => model.QuantityInStock)</th>
                                            <th>@Html.DisplayNameFor(model => model.DisplayOrder)</th>
                                            <th>@Html.DisplayNameFor(model => model.Published)</th>
                                            <th>@Html.DisplayNameFor(model => model.Deleted)</th>
                                            <th>@Html.DisplayNameFor(model => model.ProductTitleURL)</th>
                                            <th>@Html.DisplayNameFor(model => model.CreatedAt)</th>
                                            <th>@Html.DisplayNameFor(model => model.CreatedBy)</th>
                                            <th>@Html.DisplayNameFor(model => model.UpdatedAt)</th>
                                            <th>@Html.DisplayNameFor(model => model.UpdatedBy)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.ProductName)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.ShortDescription)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.Description)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.ManufacturerId)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.UnitTypeId)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.TaxRateId)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.SalePrice)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.RetailPrice)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.Point)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.ShowOnHomePage)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.ShowOnSalePage)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.WarehouseId)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.ProductTypeId)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.IsGroup)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.OwenSale)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.QuantityInStock)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.DisplayOrder)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.Published)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.Deleted)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.ProductTitleURL)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.CreatedAt)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.CreatedBy)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.UpdatedAt)
                                            </td>
                                            <td class="col-sm-10">
                                                @Html.DisplayFor(model => model.UpdatedBy)
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                            <div class="row">
                                <div class="col-md-6 text-start">
                                    <input value="Trở lại" type="submit" class="btn btn-primary mt-3 ml-2" asp-action="Index">
                                </div>
                                <div class="col-md-6 text-end">
                                    <input type="submit" value="Xóa" class="btn btn-primary mt-3 mr-2" />
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <!-- Row end -->
    </div>
</div>