@model BanHangOnline.ViewModels.CategoryViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Manager/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper-scroll">
    <!-- Content wrapper start -->
    <div class="content-wrapper">
        <!-- Row start -->
        <div class="row gutters">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="card">
                    <div class="card-body">

                        <div class="table-responsive">
                            <table id="copy-print-csv" class="table v-mitdle">
                                <thead>
                                    <tr>
                                        <th>@Html.DisplayNameFor(model => model.CategoryImages)</th>
                                        <th>@Html.DisplayNameFor(model => model.ParentId)</th>
                                        <th>@Html.DisplayNameFor(model => model.CategoryName)</th>
                                        <th>@Html.DisplayNameFor(model => model.ShortDescription)</th>
                                        <th>@Html.DisplayNameFor(model => model.Description)</th>
                                        <th>@Html.DisplayNameFor(model => model.CategoryURL)</th>
                                        <th>@Html.DisplayNameFor(model => model.CategorySeoKeywords)</th>
                                        <th>@Html.DisplayNameFor(model => model.CategoryShowHome)</th>
                                        <th>@Html.DisplayNameFor(model => model.CategoryEnable)</th>
                                        <th>@Html.DisplayNameFor(model => model.CategoryOrder)</th>
                                        <th>@Html.DisplayNameFor(model => model.CreatedAt)</th>
                                        <th>@Html.DisplayNameFor(model => model.CreatedBy)</th>
                                        <th>@Html.DisplayNameFor(model => model.UpdatedAt)</th>
                                        <th>@Html.DisplayNameFor(model => model.UpdatedBy)</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td class="col-sm-10">
                                            <div class="media-box">
                                                <img src="@Model.CategoryImages" class="media-avatar" alt="@Model.CategoryName">
                                            </div>
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.ParentId)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CategoryName)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.ShortDescription)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.Description)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CategoryURL)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CategorySeoKeywords)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CategoryShowHome)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CategoryEnable)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CategoryOrder)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CreatedAt)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.CreatedBy)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.UpdatedAt)
                                        </td>
                                        <td class="col-sm-10">
                                            @Html.DisplayFor(model => model.UpdatedBy)
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                        <div class="row">
                            <div class="col-md-6 text-start">
                                <input value="Trở lại" type="submit" class="btn btn-primary mt-3 ml-2" onclick="test()">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Row end -->
    </div>
</div>
