            url: '/DbResultJson/GetCategorys',
            type: "GET",
            dataType: "json",
            success: function(data) {
                $.each(data, function(i, result) {
                  var  txt += "<li><a href=" + result.categoryURL + ">" + result.categoryName + "</a></li>";
                    $("#listCategoryHeader").html(txt)
                    $("#listCategoryHeader1").html(txt)
                });
            }
        });
         $.ajax({
            url: '/DbResultJson/Getuser',
            type: "GET",
            dataType: "json",
            success: function(data) {
                 sdt = data.phoneNumber;
                    email = data.email;
                //$("#email").html(email);
                $("#email").html(email);
                $("#email2").html(email);
             
            }
        });
    }), 500);

</script>


